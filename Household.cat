            <selectionEntryGroup id="0cdb-f601-f9df-e109" name="Donar Qualities" publicationId="975a-00f4-pubN89746" page="48" hidden="false" collective="false" import="false">
              <modifierGroups>
                <modifierGroup>
                  <conditions>
                    <condition field="selections" scope="parent" value="0.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" childId="39d4-f743-a814-577e" type="equalTo"/>
